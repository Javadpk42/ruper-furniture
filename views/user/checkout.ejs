		<script>
		  $(document).ready(function() {
			$("#checkout-form").on("submit", function(e) {
			  e.preventDefault(); // Prevent the default form submission
		
			  // Your existing code
			  console.log("clicked");
			  var address = $("input[name=selected_address]:checked").val(); // Assuming the name attribute for the address input is 'selected_address'
			  var total = $("#totalAmount").val(); // Assuming the total amount is in an input field with the id 'totalAmount'
			  var payment = $("input[name=payment_method]:checked").val(); // Assuming the name attribute for the payment method input is 'payment_method'
			  console.log('button clicked');
		
			  console.log('Before AJAX request');
			  // Send the data using jQuery AJAX
			  $.ajax({
				type: "POST",
				url: "/place_order", // Assuming the route for placing an order is '/place_order'
				data: { selected_address: address, payment_method: payment, totalAmount: total },
				success: function(response) {
					console.log('ggggggggggggggggggg');
				  if (response.success === true) {
					console.log("entered ajax");
					  razorPayment(response.razorpayOrder)
					} else {
					  window.location.href = `/orderPlaced/${response.orderid}`
				  }
				}
			  });
			  console.log('After AJAX request');
			});
		  });
		
		  function razorPayment(order) {
			console.log("entered to razorpay");
			var options = {
			  key: "7qPA2dWay5GP1spvWde4dVy8", // Replace with your actual Razorpay key
			  amount: order.amount,
			  currency: "INR",
			  name: "Your Business Name",
			  description: "Test Transaction",
			  image: "/user/images/icons/",
			  order_id: order.id,
			  handler: function(response) {
				verifyPayment(response, order);
			  },
			  prefill: {
				name: "Your Customer's Name",
				email: "customer@example.com",
				contact: "9230566487",
			  },
			  notes: {
				address: "Your Company Address",
			  },
			  theme: {
				color: "#c96",
			  },
			};
		
			var rzp1 = new Razorpay(options);
			rzp1.open();
		  }
		
		  function verifyPayment(payment, order) {
			const amount2 = $("#totalAmount").val(); // Assuming the total amount is in an input field with the id 'totalAmount'
			$.ajax({
			  url: "/verifyPayment",
			  method: "post",
			  data: {
				payment: payment,
				amount2: amount2,
				order: order,
			  },
			  success: (response) => {
				if (response.codsuccess == true) {
				  location.href = '/order_placed';
				} else {
				  swal.fire({
					position: "center",
					icon: "error",
					title: "Payment failed",
					showConfirmButton: false,
					timer: 1500,
				  });
				}
			  },
			});
		  }
		
		</script>
		




		<script>
			function validAddress() {
  const name = document.getElementById('fullName').value;
  const mobile = document.getElementById('phone').value;
  const state = document.getElementById('state').value;
  const district = document.getElementById('district').value;
  const city = document.getElementById('city').value;
  const pincode = document.getElementById('pinCode').value;

  document.getElementById('name-error').textContent = '';
  document.getElementById('state-error').textContent = '';
  document.getElementById('mobile-error').textContent = '';
  document.getElementById('district-error').textContent = '';
  document.getElementById('city-error').textContent = '';
  document.getElementById('pincode-error').textContent = '';

  if (!name) {
      document.getElementById('name-error').textContent = 'Name field should not be empty!';
      return false;
  }
  const mobileRegex = /^[6-9]\d{9}$/;

if (!mobile || !mobileRegex.test(mobile)) {
  document.getElementById('mobile-error').textContent = 'Mobile number should be a 10-digit valid number';
  return false;
}

if (new Set(mobile).size === 1) {
  document.getElementById('mobile-error').textContent = 'Mobile number should not consist of the same digit.';
  return false;
}

  if (!state) {
      document.getElementById('state-error').textContent = 'State field should not be empty!';
      return false;
  }


  if (!district) {
      document.getElementById('district-error').textContent = 'District field should not be empty!';
      return false;
  }

  if (!city) {
      document.getElementById('city-error').textContent = 'City field should not be empty!';
      return false;
  }

  const pincodeRegex = /^\d{6}$/;

  if (!pincode || !pincode.match(pincodeRegex)) {
      document.getElementById('pincode-error').textContent = 'Pincode should be a 6-digit numeric value.';
      return false;
  }

  return true;
}

